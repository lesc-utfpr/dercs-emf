          <details key="documentation" value="Represents the action of inserting an element into an array."/>
        </eAnnotations>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The value to be inserted"/>
          </eAnnotations>
        </eStructuralFeatures>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="RemoveArrayElementAction" eSuperTypes="#//behavior/actions/ArrayAction">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents the action of removing an element from an array."/>
        </eAnnotations>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="SetArrayElementAction" eSuperTypes="#//behavior/actions/ArrayAction">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents the action of changing the value of an element in an array."/>
        </eAnnotations>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The new value of the element"/>
          </eAnnotations>
        </eStructuralFeatures>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ReturnAction" eSuperTypes="#//behavior/actions/AssignmentAction">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents the action of returning a value from a method"/>
        </eAnnotations>
        <eStructuralFeatures xsi:type="ecore:EReference" name="method" eType="#//structure/Method">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The method associated with this return action"/>
          </eAnnotations>
        </eStructuralFeatures>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ModifyStateAction" eSuperTypes="#//behavior/actions/ObjectAction">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents the action of performing a state transition on an object."/>
        </eAnnotations>
        <eStructuralFeatures xsi:type="ecore:EReference" name="relatedTransition"
            eType="#//behavior/StateTransition">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The state transition performed by the action"/>
          </eAnnotations>
        </eStructuralFeatures>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ExpressionAction" eSuperTypes="#//behavior/actions/ActionWithOutput">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents an action which evaluates a mathematical or boolean expression"/>
        </eAnnotations>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="expression" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The expression which will be evaluated"/>
          </eAnnotations>
        </eStructuralFeatures>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="AssignmentAction" eSuperTypes="#//behavior/actions/Action">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents an assignment action. A value can be assigned either to a local&#xA;varible or to an attribute of an object.&lt;br>&#xA;Assigment actions are specified within sequence diagram using the following&#xA;syntax:&#xA;&lt;ol>&#xA;  &lt;li>&lt;b>&lt;/b>&lt;/li>&#xA;  &lt;li>&lt;b>UML replay&lt;/b>, where the name of the message can be a local variable&#xA;    or an attribute of the class.&#xA;  &lt;/li>&#xA;  &lt;li>&lt;b>ASSIGN(leftSide, rightSide)&lt;/b>, where:&lt;br>&#xA;     &lt;i>leftSide&lt;/i> is the name of a local variable or an attribute&lt;br>&#xA;     &lt;i>rightSide&lt;/i> is a expression (e.g. mathematical, boolean, of string&#xA;     operation), a local variable, an attribute or a parameter.&lt;br>&#xA;  &lt;/li>&#xA;&lt;/ol>"/>
        </eAnnotations>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The value that will be assigned (If the right side is a literal value)"/>
          </eAnnotations>
        </eStructuralFeatures>
        <eStructuralFeatures xsi:type="ecore:EReference" name="destinationVariable"
            eType="#//behavior/LocalVariable">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The variable which will be assigned with the new value (if the assignment is to a local variable)"/>
          </eAnnotations>
        </eStructuralFeatures>
        <eStructuralFeatures xsi:type="ecore:EReference" name="destinationAttribute"
            eType="#//structure/Attribute">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The attribute which will be assigned with the new value (if the assignment is to an attribute)"/>
          </eAnnotations>
        </eStructuralFeatures>
        <eStructuralFeatures xsi:type="ecore:EReference" name="destinationObject"
            eType="#//structure/runtime/Object">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The object whose attribute will receive the new value (if the assignment is to an attribute)"/>
          </eAnnotations>
        </eStructuralFeatures>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="destinationArrayElement"
            eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The array element which will be assigned with the new value (if the assignment is to an array)"/>
          </eAnnotations>
        </eStructuralFeatures>
        <eStructuralFeatures xsi:type="ecore:EReference" name="resultOfAction" eType="#//behavior/actions/ActionWithOutput">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The action whose result will be assigned (If the right side is the result of an action)"/>
          </eAnnotations>
        </eStructuralFeatures>
      </eClassifiers>
    </eSubpackages>
    <eSubpackages name="events" nsURI="http://utfpr.edu.br/dercs/behavior/events"
        nsPrefix="events">
      <eClassifiers xsi:type="ecore:EEnum" name="TriggerKind">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents the way an event triggers behavior. Behavior can be triggered in&#xA;sequence, after the execution of the previous behavior. Triggers can also&#xA;start the execution of various behaviors at the same time, making them&#xA;perform their actions concurrently."/>
        </eAnnotations>
        <eLiterals name="SEQUENTIAL"/>
        <eLiterals name="PARALLEL" value="1"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="Event" abstract="true" eSuperTypes="#//structure/NamedElement">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents an event that can happen during DERTS execution, such as hardware&#xA;interruptions, state transitions, sending/receiving messages, etc."/>
        </eAnnotations>
        <eStructuralFeatures xsi:type="ecore:EReference" name="triggeredBehaviors"
            upperBound="-1" eType="#//behavior/Behavior">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The behaviors which are triggered by this event"/>
          </eAnnotations>
        </eStructuralFeatures>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="behaviorTriggerKind"
            eType="#//behavior/events/TriggerKind">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The way in which the behaviors should be triggered"/>
          </eAnnotations>
        </eStructuralFeatures>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="MessageEvent" abstract="true" eSuperTypes="#//behavior/events/Event">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents a message-related event."/>
        </eAnnotations>
        <eStructuralFeatures xsi:type="ecore:EReference" name="action" eType="#//behavior/actions/Action">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The action that caused this event"/>
          </eAnnotations>
        </eStructuralFeatures>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="SendMessageEvent" eSuperTypes="#//behavior/events/MessageEvent">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents an event which happens when a message is sent from one objet to another one."/>
        </eAnnotations>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ReceiveMessageEvent" eSuperTypes="#//behavior/events/MessageEvent">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents an event which happens when a message is received by an object."/>
        </eAnnotations>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="StateEvent" abstract="true" eSuperTypes="#//behavior/events/Event">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represent a state-related event. At the moment, there are three kinds of state events:&#xA;&lt;br> 1. Exit state event;&#xA;&lt;br> 2. State transition event;&#xA;&lt;br> 3. Enter state event;&#xA;&lt;br> The order of occurrence of these events is the same that appears above,&#xA;i.e. first an exit event happens, than a transition event, and finally an &#xA;enter event."/>
        </eAnnotations>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="EnterStateEvent" eSuperTypes="#//behavior/events/StateEvent">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents an event which happens when an object enters a new state."/>
        </eAnnotations>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ExitStateEvent" eSuperTypes="#//behavior/events/StateEvent">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents an event which happens when an object exits from a state."/>
        </eAnnotations>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="StateTransitionEvent" eSuperTypes="#//behavior/events/StateEvent">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents an event which happens when an object changes from one state to another."/>
        </eAnnotations>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="InterruptionEvent" eSuperTypes="#//behavior/events/Event">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Represents hardware interruptions that can happen during DERTS execution."/>
        </eAnnotations>
      </eClassifiers>
    </eSubpackages>
  </eSubpackages>
  <eSubpackages name="AO" nsURI="http://utfpr.edu.br/dercs/AO" nsPrefix="AO">
    <eClassifiers xsi:type="ecore:EClass" name="AspectAdaptation" abstract="true"
        eSuperTypes="#//structure/NamedElement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Represents the adaptation which an aspect applies in a determined join point.&#xA;An adaptation specifies in what way the concern's structure or behavior is &#xA;adapted. An adaptation specication is implemented by structural adaptations &#xA;or behavioral adaptations or both.&#xA;ECORE: the owner attribute was moved to the child classes, because otherwise &#xA;containment relationships would not wokr correctly"/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="owner" eType="#//AO/Aspect">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The aspect which owns this adaptation&#xA;NOTE: Due to Ecore limitations this is not automatically set when adding the Adaptation to an aspect. It must be manually set."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Aspect" eSuperTypes="#//structure/NamedElement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Represents an aspect of DERAF. An aspect is a unit of modularization of a &#xA;crosscutting concern with respect to another concern. An aspect has &#xA;adaptations that are applied into specific elements of a concern (join &#xA;points). There is two sorts of adaptations:&#xA;&lt;br>1. Structural adaptation modifies the structure of the affected concerns &#xA;by adding, removing or changing structural charateristics. &#xA;&lt;br>2. Behavioral adaptation modifies the behavior of the affected concerns&#xA;by adding, removing or changing the actions performed within the context&#xA;of a concern.&#xA;The link between adaptations and join points is called pointcut.&#xA;A pointcut can link an adaptation to several distinct join points, indicating&#xA;a relative position in which the adaptation is applied. "/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="structuralAdaptations"
          upperBound="-1" eType="#//AO/StructuralAdaptation" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="behavioralAdaptations"
          upperBound="-1" eType="#//AO/BehavioralAdaptation" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="pointcuts" upperBound="-1"
          eType="#//AO/Pointcut"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="crosscutting" upperBound="-1"
          eType="#//AO/Crosscutting" containment="true" eOpposite="#//AO/Crosscutting/associatedAspect"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="BehavioralAdaptation" eSuperTypes="#//AO/AspectAdaptation">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Represents the behavioral adaptation applied by an aspect into a concern.&#xA;A behavioral adaptation comprises a language's behavioral elements for &#xA;adapting concerns, e.g. adding new actions into a behavior, enclosing a sequence&#xA;of action in a loop, performing a check before reading an attribute, etc."/>
      </eAnnotations>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="StructuralAdaptation" eSuperTypes="#//AO/AspectAdaptation">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Represents the structural adaptation applied by an aspect into a concern.&#xA;A structural adaptation comprises a language's structural elements for &#xA;adapting concerns, e.g. add a new attribute into a class, changing the parent&#xA;of a class, removing attributes or methods, etc."/>
      </eAnnotations>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="RelativePosition">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Represents the position (relative to the join points) where an aspect applies the adaptation associated through it's pointcuts."/>
      </eAnnotations>
      <eLiterals name="BEFORE">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Indicates that the behavioral adaptation is applied before the join point"/>
        </eAnnotations>
      </eLiterals>
      <eLiterals name="AFTER" value="1">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Indicates that the behavioral adaptation is applied after the join point"/>
        </eAnnotations>
      </eLiterals>
      <eLiterals name="AROUND" value="2">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Indicates that the behavioral adaptation wraps the join point"/>
        </eAnnotations>
      </eLiterals>
      <eLiterals name="REPLACE" value="3">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Indicates that the behavioral adaptation replaces the join point"/>
        </eAnnotations>
      </eLiterals>
      <eLiterals name="ADD_NEW_FEATURE" value="4">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Indicates that the structural adaptation adds a new feature (e.g. attributes or methods) to the affected element"/>
        </eAnnotations>
      </eLiterals>
      <eLiterals name="MODIFY_STRUCTURE" value="5">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Indicates that the structural adaptation modifies the structure of the affected element"/>
        </eAnnotations>
      </eLiterals>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="JoinPoint" eSuperTypes="#//structure/NamedElement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Represents the joint points used by the aspects weaver to discover the point&#xA;where the adaptations should be applied. A join point specifies where an &#xA;aspect might insert adaptations. Thus a join point is a representation of the &#xA;selection of identiable structural or behavioral elements of the underlying &#xA;language used to capture a concern.&#xA;&lt;br>At the moment, the folowing elements can be selected:&#xA;&lt;OL>&#xA;&lt;LI>Classes;&lt;/LI>&#xA;&lt;LI>Attributes;&lt;/LI>&#xA;&lt;LI>Methods;&lt;/LI>&#xA;&lt;LI>Objects (for structural adaptation, which need runtime information);&lt;/LI>&#xA;&lt;LI>Nodes;&lt;/LI>&#xA;&lt;LI>Behaviors;&lt;/LI>&#xA;&lt;LI>The following actions:&#xA;    &lt;OL>&#xA;    &lt;LI>Sending messages;&lt;/LI>&#xA;    &lt;LI>Return;&lt;/LI>&#xA;    &lt;LI>Object creation;&lt;/LI>&#xA;    &lt;LI>Object destruction;&lt;/LI>&#xA;    &lt;/OL>&#xA;&lt;/LI>&#xA;&lt;/OL>"/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="selectedElements" upperBound="-1"
          eType="#//structure/BaseElement">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="List of all elements selected by the join point"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="model" eType="#//Model"
          eOpposite="#//Model/joinPoints">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The owner model"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="elementSelectionKind"
          eType="#//AO/ElementSelectionKind">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The kind of element selected by this join point"/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="ElementSelectionKind">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Represents the element kind that is selected by the join point."/>
      </eAnnotations>
      <eLiterals name="UNKNOWN"/>
      <eLiterals name="CLASS" value="1"/>
      <eLiterals name="ATTRIBUTE" value="2"/>
      <eLiterals name="METHOD" value="3"/>
      <eLiterals name="OBJECT" value="4"/>
      <eLiterals name="NODE" value="5"/>
      <eLiterals name="ACTION_MESSAGE_SENDING" value="100"/>
      <eLiterals name="ACTION_RETURN" value="101"/>
      <eLiterals name="ACTION_OBJECT_CREATION" value="102"/>
      <eLiterals name="ACTION_OBJECT_DESTRUCTION" value="103"/>
      <eLiterals name="BEHAVIOR" value="104"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Pointcut" eSuperTypes="#//structure/NamedElement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Represents the pointcuts used by aspects to link join points with adaptations.&#xA;If the pointcut has more than one join point, the adaptation will be applied&#xA;into all different join points respecting the indicated relative position."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="joinPoints" upperBound="-1"
          eType="#//AO/JoinPoint">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="List of join points linked with the aspect adaptation"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="aspectAdaptation" eType="#//AO/AspectAdaptation">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The aspect adaptation that should be applied into the join point"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="relativePosition" eType="#//AO/RelativePosition">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The relative position where the adaptation should be applied"/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Crosscutting" eSuperTypes="#//structure/NamedElement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Represents information inserted by an aspect in an object."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="affectedElement" eType="#//structure/BaseElement">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The element affected by the aspect"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="crosscuttingInformations"
          upperBound="-1" eType="#//AO/CrosscuttingInformation">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The information inserted by the aspect"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="associatedAspect" eType="#//AO/Aspect"
          eOpposite="#//AO/Aspect/crosscutting">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The aspect that affects the element"/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CrosscuttingInformation" eSuperTypes="#//structure/NamedElement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Represents crosscutting information, i.e. the name of an information as well as it's value."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The value of the information"/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
  </eSubpackages>
</ecore:EPackage>
